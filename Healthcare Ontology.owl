<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://www.semanticweb.org/healthcare#"
    xml:base="http://www.semanticweb.org/healthcare"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xml="http://www.w3.org/XML/1998/namespace"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://www.semanticweb.org/healthcare">
        <rdfs:label>Healthcare Clinical Ontology</rdfs:label>
        <rdfs:comment>
            Ontology describing clinical healthcare concepts including patients,
            medical staff, diagnoses, treatments, and healthcare facilities.
        </rdfs:comment>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>



    <owl:AnnotationProperty rdf:about="#author"/>
    <owl:AnnotationProperty rdf:about="#creationDate"/>
    <owl:AnnotationProperty rdf:about="#source"/>
    <owl:AnnotationProperty rdf:about="#license"/>


    <owl:Class rdf:about="#Person"/>
    <owl:Class rdf:about="#Patient">
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>
    <owl:Class rdf:about="#HealthcareProfessional">
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>
    <owl:Class rdf:about="#Doctor">
        <rdfs:subClassOf rdf:resource="#HealthcareProfessional"/>
    </owl:Class>
    <owl:Class rdf:about="#Nurse">
        <rdfs:subClassOf rdf:resource="#HealthcareProfessional"/>
    </owl:Class>
    <owl:Class rdf:about="#Hospital"/>
    <owl:Class rdf:about="#Department"/>
    <owl:Class rdf:about="#MedicalRecord"/>
    <owl:Class rdf:about="#Diagnosis"/>
    <owl:Class rdf:about="#Disease"/>
    <owl:Class rdf:about="#Treatment"/>
    <owl:Class rdf:about="#Medication"/>
    <owl:Class rdf:about="#Surgery">
        <rdfs:subClassOf rdf:resource="#Treatment"/>
    </owl:Class>
    <owl:Class rdf:about="#Appointment"/>
    <owl:Class rdf:about="#LaboratoryTest"/>

 

    <owl:ObjectProperty rdf:about="#hasPatient"/>
    <owl:ObjectProperty rdf:about="#hasDoctor"/>
    <owl:ObjectProperty rdf:about="#hasNurse"/>
    <owl:ObjectProperty rdf:about="#worksAt"/>
    <owl:ObjectProperty rdf:about="#hasDepartment"/>
    <owl:ObjectProperty rdf:about="#hasMedicalRecord"/>
    <owl:ObjectProperty rdf:about="#hasDiagnosis"/>
    <owl:ObjectProperty rdf:about="#hasDisease"/>
    <owl:ObjectProperty rdf:about="#prescribesTreatment"/>
    <owl:ObjectProperty rdf:about="#prescribesMedication"/>
    <owl:ObjectProperty rdf:about="#undergoesTreatment"/>
    <owl:ObjectProperty rdf:about="#performsSurgery"/>
    <owl:ObjectProperty rdf:about="#hasAppointment"/>
    <owl:ObjectProperty rdf:about="#attendsAppointment"/>
    <owl:ObjectProperty rdf:about="#conductsTest"/>
    <owl:ObjectProperty rdf:about="#hasTestResult"/>
    <owl:ObjectProperty rdf:about="#belongsToHospital"/>
    <owl:ObjectProperty rdf:about="#treatedBy"/>
    <owl:ObjectProperty rdf:about="#diagnosedBy"/>
    <owl:ObjectProperty rdf:about="#recordsDiagnosis"/>

 

    <owl:DatatypeProperty rdf:about="#hasName"/>
    <owl:DatatypeProperty rdf:about="#hasAge"/>
    <owl:DatatypeProperty rdf:about="#hasGender"/>
    <owl:DatatypeProperty rdf:about="#hasPatientID"/>
    <owl:DatatypeProperty rdf:about="#hasDoctorID"/>
    <owl:DatatypeProperty rdf:about="#hasHospitalName"/>
    <owl:DatatypeProperty rdf:about="#hasDepartmentName"/>
    <owl:DatatypeProperty rdf:about="#hasDiagnosisDate"/>
    <owl:DatatypeProperty rdf:about="#hasTreatmentStartDate"/>
    <owl:DatatypeProperty rdf:about="#hasTreatmentEndDate"/>
    <owl:DatatypeProperty rdf:about="#hasMedicationName"/>
    <owl:DatatypeProperty rdf:about="#hasDosage"/>
    <owl:DatatypeProperty rdf:about="#hasAppointmentDate"/>
    <owl:DatatypeProperty rdf:about="#hasTestName"/>
    <owl:DatatypeProperty rdf:about="#hasTestValue"/>
    <owl:DatatypeProperty rdf:about="#hasResultDate"/>
    <owl:DatatypeProperty rdf:about="#hasSeverity"/>
    <owl:DatatypeProperty rdf:about="#hasMedicalNotes"/>
    <owl:DatatypeProperty rdf:about="#hasLicenseNumber"/>
    <owl:DatatypeProperty rdf:about="#hasEmail"/>

</rdf:RDF>
